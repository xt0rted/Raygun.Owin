<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <add key="raygun:apiKey" value="YOUR_APP_API_KEY" />
        <add key="raygun:tags" value="nancy, sample" />
        <add key="raygun:applicationVersion" value="1.2.3.4-beta2" />
        <add key="raygun:excludeErrorsFromLocal" value="false" />
    </appSettings>

    <system.web>
        <compilation debug="true" targetFramework="4.5" />
        <httpRuntime targetFramework="4.5" />
    </system.web>
</configuration>
